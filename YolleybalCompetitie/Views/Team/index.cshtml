@model TeamViewModel

@{
    ViewData["Title"] = "index";
}

<h1>Teams</h1>

<table class="table">
    <thead>
        <tr>
            <th>Naam</th>
            <th>Competitie</th>
        </tr>
    </thead>
    <tbody>
@foreach (var team in Model.Teams) {
        <tr>
            <td>
                @team.Naam.ToString()
            </td>
            <td>
                @team.Competitie.Naam.ToString()
            </td>
        </tr>
}
    </tbody>
</table>
