@model SpelerViewModel
@{
    ViewData["Title"] = "Index";
}
<body>

    <h1>Spelers</h1>

    <table class="table">
        <tr>
            <th>
                Voornaam
            </th>
            <th>
                Achternaam
            </th>
            <th>
                Leeftijd
            </th>
            <th>
                Woonplaats
            </th>
            <th>
                Team
            </th>
            <th></th>
        </tr>

        @foreach (var speler in @Model.Spelers)
        {
    <tr>
        <td>
            @speler.Voornaam
        </td>
        <td>
            @speler.Achternaam
        </td>
        <td>
            @speler.Leeftijd
        </td>
        <td>
            @speler.Woonplaats
        </td>
        <td>
            @speler.Team.Naam
        </td>
    </tr>
        }
    </table>
</body>