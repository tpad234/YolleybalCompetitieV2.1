@model WedstrijdViewModel
@{
    ViewData["Title"] = "Index";
}
<body>

    <h1>Wedstrijden</h1>

    <table class="table">
        <tr>
            <th>
                thuisteam
            </th>
            <th>
                uitteam
            </th>
            <th>
                locatie
            </th>
            <th></th>
        </tr>

        @foreach (var item in @Model.Wedstrijden)
        {
         <tr>
            <td>
                @item[1].ToString()
            </td>
            <td>
                @item[2].ToString()
            </td>
            <td>
                @item[4].ToString()
            </td>
        </tr>
        }
    </table>
</body>